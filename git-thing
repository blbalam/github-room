#한글 폰트 지원(설치)
!apt  -qq  -y install fonts-nanum

#한글 폰트 그래프에 적용
import matplotlib.pyplot as plt
import matplotlib.font_manager as fm

fontpath = " 폰트 위치"
font = fm.FontProperties(fname=fontpath, size=10)
fm._rebuild()
